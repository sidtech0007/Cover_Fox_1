<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
<listeners>
 <listener class-name="coverfoxUtility.Listener"></listener>
</listeners>

  <test thread-count="5" name="Test_chrome">
  <parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="coverfoxTest.TC0001_CoverFox_ValidateBannerCount"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Test_edge">
  <parameter name="browser" value="edge"></parameter>
    <classes>
      <class name="coverfoxTest.TC0001_CoverFox_ValidateBannerCount"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="Test_firefox">
  <parameter name="browser" value="firefox"></parameter>
    <classes>
      <class name="coverfoxTest.TC0001_CoverFox_ValidateBannerCount"/>
    </classes>
  </test> <!-- Test -->
  
</suite> <!-- Suite -->
